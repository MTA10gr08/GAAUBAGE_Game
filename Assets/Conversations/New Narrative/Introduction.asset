    aiFace: 0
    AnimatedSprite: {fileID: 0}
    StillSprite: {fileID: 0}
  - name: '[You]'
    speech: But?
    textSpeed: 0
    aiFace: 0
    AnimatedSprite: {fileID: 0}
    StillSprite: {fileID: 0}
